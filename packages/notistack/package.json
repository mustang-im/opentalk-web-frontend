{
  "name": "@opentalk/notistack",
  "version": "2.0.8",
  "description": "Highly customizable notification snackbars (toasts) that can be stacked on top of each other",
  "main": "dist/index.js",
  "module": "dist/notistack.esm.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build --transpileOnly --entry ./src/index.js && npm run copy-types",
    "copy-types": "shx cp ./src/index.d.ts ./dist/index.d.ts",
    "prepublishOnly": "npm run build",
    "docs": "rimraf typedoc.json && typedoc --tsconfig"
  },
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.7.0",
    "@types/node": "^13.9.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-plugin-optimize-clsx": "^2.6.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "~18.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "shx": "^0.3.4",
    "tsdx": "^0.13.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "clsx": "^1.1.0",
    "hoist-non-react-statics": "^3.3.0"
  }
}
