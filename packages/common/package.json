{
  "name": "@opentalk/common",
  "version": "1.0.21",
  "author": "OpenTalk GmbH <mail@opentalk.eu>",
  "license": "EUPL-1.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.development.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=17.5"
  },
  "scripts": {
    "build": "shx rm -rf ./dist && rollup -c && shx cp -r ./build/* ./dist",
    "build-svg": "svgr --typescript -d assets/icons assets/icons/source",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-ts": "rm -rf /lib && yarn build:esm && yarn build:cjs",
    "hot-reload:remove-build": "node -e \"if (require('fs').readFileSync('../../app/hotReload/packages.tsconfig.json', {encoding: 'utf8'}).includes('@opentalk/common')){process.exit(1)}\" || shx rm -rf ./dist",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint . --ext .tsx --ext .ts",
    "before:test": "yarn install:peers",
    "install:peers": "yarn add -D @emotion/styled@^11.8.1 && yarn add -D @emotion/react@^11.10.4 ",
    "remove:peers": "yarn remove @emotion/styled @emotion/react",
    "test": "jest --watch",
    "publish:npm": "npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mui/material": "^5.10.5",
    "@reduxjs/toolkit": "^1.8.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/cli": "^6.5.1",
    "@svgr/rollup": "^6.5.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.20",
    "@types/react-dom": "18.0.6",
    "babel-jest": "^29.5.0",
    "eslint": "^8.43.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "formik": "^2.2.9",
    "formik-wizard-form": "^2.1.0",
    "i18next": "^21.9.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-svg-transformer": "^1.0.0",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-redux": "^7.2.8",
    "rollup": "^3.8.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.4",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.10.5",
    "@mui/styles": "^5.10.3",
    "date-fns": "^2.29.3",
    "formik": "^2.2.9",
    "formik-wizard-form": "^2.1.0",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-redux": "^7.2.8",
    "yup": "^0.32.11"
  },
  "publishConfig": {
    "access": "restricted",
    "@opentalk:registry": "https://git.opentalk.dev/api/v4/projects/434/packages/npm/"
  }
